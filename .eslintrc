{
  "parser": "babel-eslint",
  "extends": [
    "airbnb"
  ],
  "plugins": [
    "jest",
    "flowtype"
  ],
  "env": {
    "jest/globals": true,
  },
  "globals": {
    fetch: true
  },
  "rules": {
    "no-use-before-define": 0,
    "no-console": 0,
    "react/jsx-filename-extension": 0,
    "react/prefer-stateless-function": 0,
    "flowtype/define-flow-type": 1,
    "flowtype/use-flow-type": 1
  }
}
